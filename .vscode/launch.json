        {
            "type": "pwa-msedge",
            "runtimeExecutable": "C:/Program Files (x86)/Microsoft/Edge Dev/Application/msedge.exe",
            "request": "launch",
            "name": "Edge",
            "url": "http://localhost:5006",
            "webRoot": "${workspaceFolder}"
        }
    ]
